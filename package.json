{
  "name": "elixir-jump-to-test",
  "publisher": "samuel-pordeus",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelpordeus/elixir-jump-to-test"
  },
  "displayName": "Elixir Jump to Test",
  "description": "Navigate between Elixir files and its tests (works with Phoenix)",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.elixirJumpToTest"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.elixirJumpToTest",
        "title": "Elixir: Jump to test"
      }
    ],
    "keybindings": [
      {
        "command": "extension.elixirJumpToTest",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  }
}
